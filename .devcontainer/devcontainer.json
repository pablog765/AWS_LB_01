{
  "name": "My Terraform AWS Environment",
  "image": "mcr.microsoft.com/devcontainers/universal:latest",
  "features": {
    "ghcr.io/devcontainers/features/aws-cli:latest": {
      "version": "latest"
    },
    "ghcr.io/devcontainers/features/terraform:latest": {
      "version": "latest"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "hashicorp.terraform",
        "amazonwebservices.aws-vscode-toolkit"
      ]
    }
  }
}